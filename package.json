{
  "name": "another-webgpu-kits",
  "version": "0.1.0-alpha.8",
  "description": "A set of kits for another-webgpu library",
  "type": "module",
  "main": "./build/another-webgpu-kits.cjs",
  "module": "./build/another-webgpu-kits.module.js",
  "types": "./build/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./build/another-webgpu-kits.module.js",
      "require": "./build/another-webgpu-kits.cjs"
    },
    "./src/*": "./src/*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sergsar/another-webgpu-kits"
  },
  "files": [
    "build",
    "LICENSE",
    "package.json",
    "README.md",
    "src"
  ],
  "browserslist": [
    "> 1%, not dead, not ie 11, not op_mini all"
  ],
  "scripts": {
    "start": "npm run dev",
    "build": "rollup -c utils/build/rollup.config.js && tsc",
    "build-module": "rollup -c utils/build/rollup.config.js --configOnlyModule",
    "lint-core": "eslint src",
    "lint-utils": "eslint utils",
    "lint": "npm run lint-core",
    "lint-fix": "npm run lint-core -- --fix && npm run lint-utils -- --fix",
    "postinstall": "npx patch-package"
  },
  "keywords": [
    "another-webgpu-kits",
    "another-webgpu",
    "webgpu",
    "javascript",
    "3d",
    "canvas",
    "html5"
  ],
  "author": "sergsar",
  "license": "MIT",
  "homepage": ".",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/three": "^0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-compat": "^4.2.0",
    "eslint-plugin-html": "^8.1.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "magic-string": "^0.30.10",
    "prettier": "^3.2.5",
    "rollup": "^4.17.2",
    "three": "^0.167.1",
    "typescript": "^5.5.2"
  },
  "dependencies": {
    "another-webgpu": "^0.1.0-alpha.8"
  },
  "peerDependencies": {
    "another-webgpu": ">=0.1.0-alpha.8"
  }
}
